{
    "name": "portfolio-v5",
    "type": "module",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "cross-env MODE=development astro dev",
        "start": "astro dev",
        "build": "pnpm i18n:generate && cross-env MODE=production astro build",
        "preview": "astro preview",
        "astro": "astro",
        "format": "prettier --write \"./src/**/*.{astro,html,js,jsx,md,mdx,svelte,ts,tsx,vue}\"",
        "i18n:generate": "pnpm script:clean-pages && astro-i18next generate && pnpm script:rename-pages && pnpm script:fix-pages-imports && pnpm script:change-language-in-dynamic-pages && prettier --write \"./src/pages/**/*.astro\"",
        "script:rename-pages": "node ./scripts/rename-pages.js",
        "script:clean-pages": "node ./scripts/clean-pages.js",
        "script:fix-pages-imports": "node ./scripts/fix-pages-imports.js",
        "script:change-language-in-dynamic-pages": "node ./scripts/change-language-in-dynamic-pages.js"
    },
    "dependencies": {
        "@astrojs/mdx": "^0.12.1",
        "@astrojs/prefetch": "^0.1.1",
        "@astrojs/react": "^1.2.2",
        "@astrojs/sitemap": "^1.0.0",
        "@astrojs/tailwind": "^2.1.3",
        "@headlessui/react": "^1.7.4",
        "@iconify/react": "^4.0.1",
        "@mdx-js/react": "^2.1.5",
        "@nanostores/react": "^0.4.0",
        "@swup/scroll-plugin": "^1.3.3",
        "@swup/theme": "^1.0.7",
        "astro": "^1.6.13",
        "astro-i18next": "1.0.0-beta.14",
        "astro-seo": "^0.6.0",
        "clsx": "^1.2.1",
        "cross-env": "^7.0.3",
        "framer-motion": "^8.0.2",
        "html-react-parser": "^3.0.4",
        "i18next": "^22.0.6",
        "i18next-browser-languagedetector": "^7.0.1",
        "i18next-http-backend": "^2.0.2",
        "jsdom": "^20.0.3",
        "nanostores": "^0.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^12.1.1",
        "react-lazy-load-image-component": "^1.5.6",
        "react-medium-image-zoom": "^5.1.2",
        "react-remark": "^2.1.0"
    },
    "devDependencies": {
        "@swup/a11y-plugin": "^2.1.0",
        "@swup/head-plugin": "^1.2.1",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.8",
        "@types/jsdom": "^20.0.1",
        "@types/node": "^18.11.10",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@types/react-lazy-load-image-component": "^1.5.2",
        "netlify-plugin-checklinks": "^4.1.1",
        "prettier": "^2.8.0",
        "prettier-plugin-astro": "^0.7.0",
        "prettier-plugin-tailwindcss": "^0.2.0",
        "swup": "^2.0.19",
        "tailwindcss": "^3.0.24"
    }
}
